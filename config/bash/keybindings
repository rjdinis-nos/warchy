# Keybindings
bind -x '"\C-@": warchy-launcher'                                                   # Application laucher
bind -x '"\C-k": warchy-shortcuts'                                                  # Show key shorcuts
bind -x '"\e\C-@": warchy-commands'                                                 # Run cli command snippets
bind -x '"\em": tmux'                                                               # Tmux
bind -x '"\ef": yazi $PWD'                                                          # File Manager
bind -x '"\eu": dua i /'                                                            # Disk Usage
bind -x '"\ed": lazydocker'                                                         # Docker (LazyDocker)
bind -x '"\eg": lazygit'                                                            # Git (LazyGit)
bind -x '"\en": nvim'                                                               # Neovim
bind -x '"\ec": code .'                                                             # Vscode
bind -x '"\et": warchy-btop'                                                        # Activity (btop)
bind -x '"\ea": xdg-open https://chatgpt.com/?q="$(wl-paste | urlencode)"'          # Search copy text in chatgtp
bind -x '"\eb": xdg-open https://www.google.com/search?q="$(wl-paste | urlencode)"' # Search copy text in browser
